extends layout

block content
    h1= title
    p Welcome to our test lol

    // Wrapper
    #wrapper

        // USER INFO
        #userInfo
            h2 User Info
            p
                strong TRANSID:
                |  <span id='userInfoTRANSID'></span>
                br
                strong RCPTID:
                |  <span id='userInfoRCPTID'></span>
                br
                strong EMAIL:
                |  <span id='userInfoEMAIL'></span>
                br
                strong EVENT:
                |  <span id='userInfoEVENT'></span>
                br
                strong TIMESTAMP:
                |  <span id='userInfoTIMESTAMP'></span>
                br
                strong RESPONSE:
                |  <span id='userInfoRESPONSE'></span>
                br
                strong Url:
                |  <span id='userInfoUrl'></span>
                br
                strong SUBJECT:
                |  <span id='userInfoSUBJECT'></span>
                br
                strong FROM ADDRESS:
                |  <span id='userInfoADDRESS'></span>
                br
                strong SIZE:
                |  <span id='userInfoSIZE'></span>
                br
                strong TAGS:
                |  <span id='userInfoTAGS'></span>
                br
                strong BOUNCE_TYPE:
                |  <span id='userInfoBOUNCE_TYPE'></span>

        // /USER INFO

        // USER LIST
        h2 User List
        #userList
            table
                thead
                    th TRANSID
                    th Email
                    th Delete
                   
                tbody
        // /USER LIST

        // ADD USER
        h2 Add User
        #addUser
            fieldset
                input#inputUserTRANSID(type='text', placeholder='TRANSID')
                input#inputUserRCPTID(type='text', placeholder='RCPTID')
                br
                input#inputUserEMAIL(type='text', placeholder='EMAIL')
                input#inputUserEVENT(type='text', placeholder='EVENT')
                br
                input#inputUserTIMESTAMP(type='text', placeholder='TIMESTAMP')
                input#inputUserRESPONSE(type='text', placeholder='RESPONSE')
                br
                input#inputUserurl(type='text', placeholder='url')
                input#inputUserSUBJECT(type='text', placeholder='SUBJECT')
                br
                input#inputUserFROMADDRESS(type='text', placeholder='FROMADDRESS')
                input#inputUserSIZE(type='text', placeholder='SIZE')
                br
                input#inputUserTAGS(type='text', placeholder='TAGS')
                input#inputUserBOUNCE_TYPE(type='text', placeholder='BOUNCE_TYPE')
                button#btnAddUser Add User
        // /ADD USER

    // /WRAPPER